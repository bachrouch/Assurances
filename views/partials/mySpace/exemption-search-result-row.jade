td(data-values='reference')
  a(href!= '#exemption/<%= reference %>/consult', title!= '<%= reference %>')
    button.btn.btn-link(type='button') <%= reference %>
td(data-values='pos')!= '<%= getPos() %>'
td(data-values='creationDate')!= '<%= _.beautifyDate(creationDate) %>'
td(data-values='closingDate')!= '<%= _.beautifyDate(closingDate) %>'
td(data-values='status')!= '<%= updateStatus() %>'
td(data-values='type')!= '<%= updateExemptionType() %>'
td(data-values='num')
  a(href!= 'finance#payment/<%= getNum() %>/consult', title!= '<%= getNum() %>')
    button.btn.btn-link(type='button') <%= getNum() %>